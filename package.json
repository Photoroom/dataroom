{
  "name": "dataroom",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "node": "22.14.0",
    "npm": "10.9.2"
  },
  "scripts": {
    "dev": "vite --port 3000 --strictPort --host localhost",
    "build": "vite build --emptyOutDir",
    "lint": "eslint 'frontend/**/*.{js,jsx,ts,tsx}' --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint 'frontend/**/*.{js,jsx,ts,tsx}' --fix",
    "format": "prettier --write frontend/src/**/*.{js,jsx,ts,tsx,css}",
    "format:check": "prettier --check frontend/src/**/*.{js,jsx,ts,tsx,css}",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "orval:generate": "orval",
    "orval:watch": "orval --watch",
    "check": "npm run typecheck && npm run lint && npm run format:check"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.0.6",
    "@tailwindcss/typography": "^0.5.16",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.20.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "fast-glob": "^3.3.3",
    "globals": "^16.3.0",
    "orval": "^7.11.2",
    "postcss": "^8.5.2",
    "postcss-nested": "^7.0.2",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "vite": "^6.1.0"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.4",
    "@heroicons/react": "^2.2.0",
    "@tanstack/react-query": "^5.66.0",
    "axios": "^1.7.9",
    "debounce": "^2.2.0",
    "deepscatter": "^2.15.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-hot-toast": "^2.5.1",
    "react-router-dom": "^7.1.5",
    "tailwind-merge": "^3.0.1"
  }
}
